import lexer::*;
import parser::*;
import emitter::*;

main :: fn() {
  mut lexer := elexify::Lexer::new(
    Some(operators()),
    Some(keywords()),
    Some(List!<str>.{}),
  );

  mut state := elexify::State::from_file("sample.b");
  lexer.push_state(state);

  program := parse_program(&mut lexer);

  mut emitter := Emitter.{};
  emitter.visit_program(program);

}